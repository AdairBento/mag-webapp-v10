{
  "name": "@mag/api",
  "version": "10.0.0",
  "description": "MAG WEB APP V10 - API",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev --name init",
    "db:deploy": "prisma migrate deploy && prisma generate",
    "lint": "eslint \"src/**/*.ts\" --report-unused-disable-directives",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"{src,tests}/**/*.{ts,tsx,js,json,md}\"",
    "test": "vitest run --coverage --passWithNoTests",
    "prepare": "husky",
    "seed": "node -r dotenv/config scripts/seed.js dotenv_config_path=.env",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "seedmin": "node -r dotenv/config scripts/seed_minimo.js dotenv_config_path=.env",
    "dev:3002": "cross-env PORT=3002 tsx watch src/server.ts",
    "test:watch": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "16.6.1",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "mustache": "^4.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.11",
    "@types/mustache": "^4.2.6",
    "@types/node": "^20.19.17",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "^2.1.9",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vitest": "^2.0.5"
  }
}
